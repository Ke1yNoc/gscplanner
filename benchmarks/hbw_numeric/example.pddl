(define (problem example)
  (:domain hydraulic_blocks_world)

  (:objects A B - block)

  (:init
   (handempty)
   (on_piston A)
   (clear A)
   (in A c0)
   (on_piston B)
   (clear B)
   (in B c1)
   (clear_piston c2)
   (= (weight A) 1)
   (= (weight B) 2)
   (= (weight_on c0) 1)
   (= (weight_on c1) 2)
   (= (weight_on c2) 0)
   (= (height c0) 5)
   (= (height c1) 5)
   (= (height c2) 5)
   (= (volume) 4)
   (= (area c0) 1)
   (= (area c1) 2)
   (= (area c2) 3)
   (= (total_area) 6)
   )

  (:goal (and
	  (>= (+ (* (/ 1 (total_area)) (volume))
		 (+ (* (/ 1 (total_area)) (weight_on c1))
		    (- (* (/ 1 (total_area)) (weight_on c2))
		       (* (/ (- (total_area) (area c0))
			     (* (total_area) (area c0)))
			  (weight_on c0)))))
	      0)
	  (on B A)))
  )
